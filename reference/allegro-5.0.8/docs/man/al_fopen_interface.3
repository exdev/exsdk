.TH al_fopen_interface 3 "" "Allegro reference manual"
.SH NAME
.PP
al_fopen_interface - Allegro 5 API
.SH SYNOPSIS
.IP
.nf
\f[C]
#include\ <allegro5/allegro.h>

ALLEGRO_FILE\ *al_fopen_interface(const\ ALLEGRO_FILE_INTERFACE\ *drv,
